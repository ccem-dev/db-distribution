{
  "name": "db-distribution",
  "version": "1.0.0",
  "description": "Node api for static database distribution",
  "main": "app.js",
  "scripts": {
    "test": "nyc --reporter=html --reporter=text mocha --recursive",
    "unit": "mocha test/**/*.js",
    "dev": "node --max-old-space-size=1024 --optimize-for-size app.js",
    "gulp": "gulp",
    "start": "npm run gulp browser-sync",
    "production": "npm install && npm test && npm prune --production"
  },
  "nyc": {
    "include": [
      "app/**/*.js",
      "test/**/*-spec.js"
    ],
    "extension": [
      ".js"
    ],
    "all": true,
    "require": [],
    "reporter": [
      "text-summary",
      "html"
    ],
    "sourceMap": true,
    "instrument": true
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "express-validator": "6.1.1",
    "consign": "0.1.6",
    "express": "4.16.4",
    "express-list-endpoints": "4.0.1",
    "mongoose": "5.3.8",
    "multer": "1.4.1"
  },
  "devDependencies": {
    "gulp": "3.9.1",
    "mocha": "6.1.4",
    "chai": "4.2.0",
    "sinon": "^7.5.0",
    "nyc": "14.1.1"
  }
}
